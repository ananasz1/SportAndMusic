<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vikischmideg.sportandmusic">

    <application
        android:allowBackup="true"
        android:icon="@drawable/nowplaying"
        android:label="@string/app_name"
        android:roundIcon="@drawable/nowplaying"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RunActivity"
            android:label="Run" />
        <activity
            android:name=".GymActivity"
            android:label="Gymnastics" />
        <activity
            android:name=".YogaActivity"
            android:label="Yoga" />
        <activity android:name=".LibraryActivity"
            android:label="Library"/>
        <activity android:name=".NowPlayingActivity"
            android:label="Now Playing"></activity>
    </application>

</manifest>